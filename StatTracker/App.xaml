<Application
    x:Class="McKnight.StatTracker.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:McKnight.StatTracker">
    <Application.Resources>
        <ResourceDictionary>
            <local:DateFormatConverter x:Key="DateToString"/>
            <local:PersonNameConverter x:Key="PersonConverter"/>
            <local:FranchiseConverter x:Key="FranchiseConverter" />
            <x:String x:Key="LongDate">dddd, dd MMMM yyyy</x:String>
        </ResourceDictionary>
    </Application.Resources>
</Application>
